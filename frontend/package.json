{
  "name": "concert-manager-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "validate:schema": "cd .. && node scripts/validate-db-schema.js",
    "dev": "vite",
    "build": "npm run validate:schema && vite build",
    "preview": "vite preview",
    "analyze": "vite-bundle-visualizer",
    "deploy:dev": "npm run build && npx wrangler pages deploy dist --project-name longweekend-bandcrawl --branch dev",
    "deploy:prod": "npm run build && npx wrangler pages deploy dist --project-name longweekend-bandcrawl --branch main",
    "psi": "node scripts/run-psi.mjs",
    "psi:dev": "npm run psi -- --url https://dev.longweekend-bandcrawl.pages.dev",
    "psi:prod": "npm run psi -- --url https://lwbc.dredre.net",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:a11y": "vitest run src/test/a11y.test.jsx",
    "test:coverage": "vitest run --coverage",
    "lighthouse": "lhci autorun",
    "quality": "npm run lint && npm run format:check && npm run test && npm run build"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@simplewebauthn/browser": "^2.0.0",
    "@simplewebauthn/server": "^8.3.0",
    "otplib": "^12.0.1",
    "prop-types": "^15.8.1",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "react-swipeable": "^7.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.37.0",
    "@lhci/cli": "^0.15.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.11.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "psi": "^4.1.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.0",
    "vite": "^5.0.8",
    "vite-bundle-visualizer": "^1.0.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.2"
  }
}
